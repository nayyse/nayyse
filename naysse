<script>
    $(function() {
        var Accordion = function(el, multiple) {
            this.el = el || {};
            this.multiple = multiple || false;
            var links = this.el.find('.top-link-btn');
            links.on('click', {el: this.el, multiple: this.multiple}, this.dropdown)
        }

        Accordion.prototype.dropdown = function(e) {
            var $el = e.data.el;
                $this = $(this),
                $next = $this.parent().next();            

            $next.slideToggle();
            $this.parent().parent().toggleClass('open');

            if (!e.data.multiple) {
                $el.find('.main-submenu').not($next).slideUp().parent().removeClass('open');
            };
        }	

        var accordion = new Accordion($('.mobile-navlist'), false);
    });
</script>

<script>
    $(function(){let timeout;

        // Enabled these features only outside of Design Mode
        // Init variables
        const top_level_links = $('a.top-level-link');
        const action_menu = $('ul.menu-actions li a');
        const subnav_links = $('ul.main-submenu li a.subnav-link');

        // Top Level Link Mouse Enter event
        // Open mega-menu and set data attributes
        top_level_links.on('mouseenter', function(){
            const current_link = $(this);
            if (timeout != null) { clearTimeout(timeout); }

            // Store the timeout handle
            // Prevents showing menu too quickly if mouse lingers just a bit
            // Otherwise mega menu flashes in and out
            timeout = setTimeout(function () {
              top_level_links.removeClass('active');
              top_level_links.attr('aria-expanded', false);
              current_link.addClass('active');
              current_link.attr('aria-expanded', true);
            }, 250);
        });

        top_level_links.on('mouseleave', function(){
          // Clear the timeout handle
          if (timeout != null) { 
            clearTimeout(timeout);
            timeout = null;
          }
        });

        // Top Level Link Mouse Leave event
        // Close mega-menu and set data attributes
        $('.mega-menu-wrapper').on('mouseleave', function(){
            top_level_links.removeClass('active');
            top_level_links.attr('aria-expanded', false);            
        });

        // Accessibility: On Topnav Link Focus
        // Open mega-menu and set data attributes
        top_level_links.on('focus', function(){
            top_level_links.removeClass('active');
            top_level_links.attr('aria-expanded', false);
            $(this).addClass('active');
            $(this).attr('aria-expanded', true);
        });     
        
        // Accessibility: On Action menu Focus
        // Close mega-menu and set data attributes
        action_menu.on('focus', function(){
            top_level_links.removeClass('active');
            top_level_links.attr('aria-expanded', false);
        });

        // On Action menu Hover
        // Close mega-menu and set data attributes
        action_menu.on('mouseenter', function(){
            top_level_links.removeClass('active');
            top_level_links.attr('aria-expanded', false);
        });

        // On Link without submenu Hover 
        // Close mega-menu and set data attributes
        $('.normal-link').on('mouseenter', function(){
            top_level_links.removeClass('active');
            top_level_links.attr('aria-expanded', false);
        });

        // On Escape Key press
        // Close mega-menu and set data attributes
        $(document).keyup(function(e) {
            if (e.keyCode === 27) {
              top_level_links.removeClass('active');
              top_level_links.attr('aria-expanded', false);

              // Close mobile nav if small desktop device width
              if ($('.mobile-menu').hasClass('menu--open') || $('.search-sidebar').hasClass('search--open')) {
                $('.mobile-menu').removeClass('menu--open');
                $('.search-sidebar').removeClass('search--open');
                $('.drawer-overlay').removeClass('show');
                $('.search-btn').removeClass('active');
                $('#mobile-toggle').removeClass('hide-toggle');
                $('body, html').removeClass('sidebar--open');
                $('button.slideshow__pause').click();
              }              
            }
        });

        // On Subnav Link Hover Set the MegaMenu Media element
        subnav_links.on('mouseenter', function(){
            setMegaMenuMedia($(this));            
        });

        // Accessibility: On Subnav Link Focus Set the MegaMenu Media element
        subnav_links.on('focus', function(){
            setMegaMenuMedia($(this));
        });

        // Detect when user goes back to Tab
        // Close mega-menu and set data attributes
        $(window).focus(function() {
            top_level_links.removeClass('active');
            top_level_links.attr('aria-expanded', false);
        });
    });

    // Set the MegaMenu Media element of the Subnav links
    function setMegaMenuMedia(link_to) {
        const linkedToWrapper = $(`.${link_to.data('linkedto')}`).find('.media-content-wrapper');
        const link_media_src = link_to.data('src');
        const link_media_desc = link_to.data('desc');

        if (link_media_src !== undefined) {       
            if (linkedToWrapper.has('img')) {
                if (linkedToWrapper.find('img').attr('src') !== link_media_src) {
                    linkedToWrapper.find('img').attr('src', link_media_src);
                    linkedToWrapper.find('.media-description p').text(link_media_desc);
                }
            }
        }
    }
</script>

<!-- Search sidebar -->
<div class="search-sidebar">
    <div class="inner-search">
        <div class="search-sidebar-header">
            <a href="#" class="close-search-btn">
                <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-close" viewBox="0 0 37 40"><path d="M21.3 23l11-11c.8-.8.8-2 0-2.8-.8-.8-2-.8-2.8 0l-11 11-11-11c-.8-.8-2-.8-2.8 0-.8.8-.8 2 0 2.8l11 11-11 11c-.8.8-.8 2 0 2.8.4.4.9.6 1.4.6s1-.2 1.4-.6l11-11 11 11c.4.4.9.6 1.4.6s1-.2 1.4-.6c.8-.8.8-2 0-2.8l-11-11z"></path></svg>
            </a>
        </div>
        <div class="search-bar-form">            
            <div class="predictive-search-wrapper">
  <form class="predictive-search" action="/search" data-redirect="/search" siq_id="autopick_2759">
    <div class="input-with-btn-icon">
      <input id="searchInput" class="form-control me-2 border-0 search-box" type="search" name="q" placeholder="Search" autocomplete="off">
      <i class="fa fa-times-circle clear-query"></i>
    </div>
  </form>
  <div class="result_box">
    <h3 class="suggestion-title">
      Product suggestions
      <i class="fa fa-circle-o-notch fa-rotate-90 fa-spin fa-loading-icon"></i>
    </h3>

    <ul class="list-group" id="searchResults">
    </ul>
  </div>
</div>

<script>
$(function(){
  const search_form = $('form.predictive-search');
  const search_input = $('#searchInput');
  const search_results = $("#searchResults");
  const search_wrapper = $('.predictive-search-wrapper');
  const loading_icon = $('i.fa-loading-icon');

  search_input.keyup(function(){
    var q = search_input.val();
    var b = '&resources[type]=product,collection&resources[options][fields]=title,product_type,variants.title,tag';
    const full_search_link = `/search?q=${q}&type=product&product_type:dress&options[prefix]=last`;
    search_wrapper.addClass('show');
    loading_icon.removeClass('hide');

    if (q.length > 3) {
      $.ajax('/search/suggest.json?q=' + q + b,{
        type: 'GET',
        dataType: 'json', // added data type
        success: function(response) {
          var productSuggestions = response.resources.results.products;
          if (productSuggestions.length > 0) {
            search_results.empty()
            
            $(productSuggestions).each(function() {
              if (this.type === 'dress' || this.type === 'accessory') {
                search_results.append(
                  '<li class="list-group-item list-group-item-action"><a class="list-card-link" href="' + this.url + '"><div class="image-parent"><img class="img-fluid lazyload" loading="lazy" alt="'+ this.title +'" src="' + this.featured_image.url + '"/></div><div class="product-details"><h4>'+ this.title +'</h4><p class="price">' + theme.Currency.formatMoney(this.price, '$') + '</p><p class="desc">' + truncateText($(this.body).text(), 150) + '</p></div></a></li>'
                );                                
              }              
            });
            
            search_results.append('<li class="list-group-item list-group-item-action"><a class="btn btn--secondary full-search-link" href="' + full_search_link + '">Search for &ldquo;' + q + '&rdquo; <i class="fa fa-angle-right"></i></a><li>');                                  

            setTimeout(function(){
              loading_icon.addClass('hide');
            }, 500);
          } else {
            search_results.empty().append(
                '<li class="list-group-item list-group-item-action no-result-found">No Results Found</li>'
            );
            
            setTimeout(function(){
              loading_icon.addClass('hide');
            }, 500);
          }
        },
        error: function(response) {
          search_results.empty().append(
              '<li class="list-group-item list-group-item-action no-result-found">An error occured. Please try again.</li>'
          );
          loading_icon.addClass('hide');
        }
      });
    } else {
      if (q.length === 0) {
        search_wrapper.removeClass('show');
        search_results.empty();
      }
    }

    search_form.attr('data-redirect', full_search_link);
  });

  $('i.clear-query').on('click', function(){
    search_input.val('');
    search_wrapper.removeClass('show');
    search_results.empty();
  });

  search_input.keypress(function(e){
    var keycode = (e.keyCode ? e.keyCode : e.which);
    if(keycode === 13){
      e.preventDefault();
      const redirect_path = search_form.attr('data-redirect') !== '/search' ? search_form.attr('data-redirect') : `/search?q=${search_input.val()}`;      
      window.location = redirect_path;
      return false;
    }    
  });
})

function truncateText(text, length) {
  if (text.length <= length) {
    return text;
  }

  return text.substr(0, length) + '\u2026'
}
</script>
        </div>        
    </div>
</div>

