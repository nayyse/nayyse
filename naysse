    <script src="//cdn.shopify.com/s/files/1/0032/0562/7968/t/35/assets/codeboxx-amsale.js?v=111576626512210917231662466183" defer="defer"></script>
    <script src="//cdn.shopify.com/s/files/1/0032/0562/7968/t/35/assets/navigation.main.js?v=99819596367983810021663188463" defer="defer"></script>

    <!-- Import scripts per Template type -->
    
    <script src="//cdn.shopify.com/s/files/1/0032/0562/7968/t/35/assets/index-homepage.js?v=109420436698157233691662125899" defer="defer"></script>

    <script>
  window.onload = function() {
    /**
      * Module to ajaxify all add to cart forms on the page.
      *
      * Copyright (c) 2015 Caroline Schnapp (11heavens.com)
      * Dual licensed under the MIT and GPL licenses:
      * http://www.opensource.org/licenses/mit-license.php
      * http://www.gnu.org/licenses/gpl.html
    */

    // Get current product collection
    var product_collection = null;
    var product_tags = null;
    // Add event listener to close modal
    var md_modal_close_btn = $('.md-modal .close-btn');
    $(md_modal_close_btn).on('click', function(){
      $('.md-modal').removeClass('md-show');
      $('html,body').css("overflow-y", "initial");
      window.location.href = '/cart';
    });
    $('#cart-modal-overlay').not('.md-modal').on('click', function(){
      $('.md-modal').removeClass('md-show');
      $('html,body').css("overflow-y", "initial");
      window.location.href = '/cart';
    });

    Shopify.AjaxifyCart = (function($) {
      // Some configuration options.
      // I have separated what you will never need to change from what
      // you might change.

      var _config = {
        // What you might want to change
        addToCartBtnLabel:             'Add to Bag',
        addedToCartBtnLabel:           'Thank you!',
        addingToCartBtnLabel:          'Adding...',
        soldOutBtnLabel:               'Sold Out',
        howLongTillBtnReturnsToNormal: 1000, // in milliseconds.
        cartCountSelector:             '#CartCount',
        cartTotalSelector:             '#cart-price',
        // 'aboveForm' for top of add to cart form, 
        // 'belowForm' for below the add to cart form, and 
        // 'nextButton' for next to add to cart button.
        feedbackPosition:              'nextButton',
        // What you will never need to change
        addToCartBtnSelector:          '[type="submit"]',
        addToCartFormSelector:         'form[action="/cart/add"]',
        shopifyAjaxAddURL:             '/cart/add.js',
        shopifyAjaxCartURL:            '/cart.js'
      };

      var _setText = function($button, label) {
        if ($button.children().length) {
          $button.children().each(function() {
            if ($.trim($(this).text()) !== '') {
              $(this).text(label);
            }
          });
        } else {
          $button.val(label).text(label);
        }
      }; // End _setText

      var _init = function() {   
        $(document).ready(function() {          
          $(_config.addToCartFormSelector).submit(function(e) {
            e.preventDefault();
            
            var $addToCartForm = $(this);
            var $addToCartBtn = $addToCartForm.find(_config.addToCartBtnSelector);
            _setText($addToCartBtn, _config.addingToCartBtnLabel);
            $addToCartBtn.addClass('disabled').prop('disabled', true);
            
            // Add to cart.
            $.ajax({
              url: _config.shopifyAjaxAddURL,
              dataType: 'json',
              type: 'post',
              data: $addToCartForm.serialize(),
              success: function(itemData) {
                // Re-enable add to cart button.
                $addToCartBtn.addClass('inverted');
                _setText($addToCartBtn, _config.addedToCartBtnLabel);
                
                window.setTimeout(function(){
                  $addToCartBtn.prop('disabled', false).removeClass('disabled').removeClass('inverted');
                  _setText($addToCartBtn,_config.addToCartBtnLabel);
                }, _config.howLongTillBtnReturnsToNormal);

                // Update cart count and show cart link.
                $.getJSON(_config.shopifyAjaxCartURL, function(cart) {
                  if (_config.cartCountSelector && $(_config.cartCountSelector).size()) {
                    var value = $(_config.cartCountSelector).html() || '0';
                    $(_config.cartCountSelector).html(value.replace(/[0-9]+/,cart.item_count)).removeClass('hidden-count');
                  }

                  if (_config.cartTotalSelector && $(_config.cartTotalSelector).size()) {
                    if (typeof Currency !== 'undefined' && typeof Currency.moneyFormats !== 'undefined') {
                      var newCurrency = '';
                      
                      if ($('[name="currencies"]').size()) {
                        newCurrency = $('[name="currencies"]').val();
                      } else if ($('#currencies span.selected').size()) {
                        newCurrency = $('#currencies span.selected').attr('data-currency');
                      }

                      if (newCurrency) {
                        $(_config.cartTotalSelector).html('<span class=money>' + Shopify.formatMoney(Currency.convert(cart.total_price, "USD", newCurrency), Currency.money_format[newCurrency]) + '</span>');
                      } else {
                        $(_config.cartTotalSelector).html(Shopify.formatMoney(cart.total_price, "${{amount}}"));
                  	  }
                    } else {
                  	  $(_config.cartTotalSelector).html(Shopify.formatMoney(cart.total_price, "${{amount}}"));
                    }
                  };

                  var swatch_obj = product_collection.find(o => o.handle === 'swatch');
                  // Check if swatch or not
                  if (swatch_obj == undefined) {
                    // If dress product, populate the modal infos
                    populateCartInfos(product_tags);
                  } else {
                    // If swatch, only show feedback
                    // Success feedback                    
                    displayUserFeedbackMessage('success', 'Success!', 'Product added to Bag.');
                  }
                });
              }, 
              error: function(XMLHttpRequest) {
              	if (response.slice(0,4) === 'All ') {
                  $addToCartBtn.prop('disabled', false);
                  _setText($addToCartBtn, _config.soldOutBtnLabel);
                  $addToCartBtn.prop('disabled',true);
                } else {
                  $addToCartBtn.prop('disabled', false).removeClass('disabled');
                  _setText($addToCartBtn, _config.addToCartBtnLabel);
                }

                //Success override feedback
                displayUserFeedbackMessage('error', 'An error occured', 'Please refresh page or contact<br>Site Administrator if error persists.');
              }
            });   

            return false;    
          });
        });
      }; // End _init
      
      return {
        init: function(params) {
          // Configuration
          params = params || {};
          // Merging with defaults.
          $.extend(_config, params);
          // Action
          $(function() {
            _init();
          });
        },
        getConfig: function() {
          return _config;
        }
      }  
    })(jQuery); // End Shopify.AjaxifyCart

    Shopify.AjaxifyCart.init();
  } // End window.onload

  //Populate cart infos
  var popupmodal = $('#cart_modal');
  function populateCartInfos(product_tags) {
    //Get variant ID and product handle name
    var current_variant_id = gup('variant', window.location.href);
    var current_handle_name = $('button[type=submit].product-form__cart-submit').attr('data-product-title');
    var is_axy = false;

    // Check if AxY combo dress
    var axy_obj = product_tags.find(o => o === 'amsale-x-you');
    if (axy_obj != undefined) {
      is_axy = true;
    }

    jQuery.getJSON('/cart', function(cart) {
      var current_added_product = cart.items[0];
      var current_product_color = "";
      var current_product_size = "";
      var current_product_length = "";
      var axy_combo_title = "";
      var axy_combo_bodice_size = "";
      var axy_combo_skirt_size = "";
      var axy_combo_skirt_length = "";
      var axybm_combo_dress_size = "";
      var product_quantity = 1;
      var product_price = (parseInt(current_added_product.price) / 100);      
      var product_total = 0;

      if (!is_axy) {
        // Get options value
        for (var i = 0; i < current_added_product.options_with_values.length; i++) {
          if (current_added_product.options_with_values[i].name.toLowerCase() == "color") {
            current_product_color = current_added_product.options_with_values[i].value;
          }

          if (current_added_product.options_with_values[i].name.toLowerCase() == "size") {
            current_product_size = current_added_product.options_with_values[i].value;
          }

          if (current_added_product.properties != null && current_added_product.properties['Size'] != "") {
            current_product_size = current_added_product.properties['Size'];
          }

          if (current_added_product.options_with_values[i].name.toLowerCase() == "dress length") {
            current_product_length = current_added_product.options_with_values[i].value;
          }            
        }

        // Set values in modal placeholders
        $(popupmodal).find('span.cart-modal-product-color').html(current_product_color);
        $(popupmodal).find('span.cart-modal-product-size').html(current_product_size);
        $(popupmodal).find('span.cart-modal-product-dress-length').html(current_product_length);
      } else {
        axy_combo_title = current_added_product.product_title;        

        // AxY BM
        if (current_added_product.properties != null && current_added_product.properties['Color'] != "") {
          current_product_color = current_added_product.properties['Color'];
        }

        if (current_added_product.properties != null && current_added_product.properties['Dress Size'] != "") {
          current_product_size = current_added_product.properties['Dress Size'];
        }

        // AxY Bridal
        if (current_added_product.properties != null && current_added_product.properties['Bodice Size'] != "") {
          axy_combo_bodice_size = current_added_product.properties['Bodice Size'];
        }

        if (current_added_product.properties != null && current_added_product.properties['Skirt Size'] != "") {
          axy_combo_skirt_size = current_added_product.properties['Skirt Size'];
        }

        if (current_added_product.properties != null && current_added_product.properties['Skirt Length'] != "") {
          axy_combo_skirt_length = current_added_product.properties['Skirt Length'];
        }
        
        // Set values in modal placeholders 
        $(popupmodal).find('tr.detail-line-name td.product-name').empty().html(axy_combo_title);
        $(popupmodal).find('span.cart-modal-product-color').html(current_product_color);
        $(popupmodal).find('span.cart-modal-product-bodice-size').html(axy_combo_bodice_size);
        $(popupmodal).find('span.cart-modal-product-skirt-size').html(axy_combo_skirt_size);
        $(popupmodal).find('span.cart-modal-product-skirt-length').html(axy_combo_skirt_length);
        $(popupmodal).find('span.cart-modal-product-size').html(current_product_size);
      }

      // Set quantity only if input is present
      if ($('input[name=quantity]').val() != undefined) {
        product_quantity = parseInt($('input[name=quantity]').val());
      }
      
      // Set values in modal placeholders      
      $(popupmodal).find('span.cart-modal-product-price').html(product_price.toFixed(2));
      $(popupmodal).find('span.cart-modal-product-qty').html(product_quantity);
      
      // Set total times quantity
      product_total = product_price * product_quantity;
      $(popupmodal).find('span.cart-modal-product-total').html(product_total.toFixed(2));
      
      // Hide lines if values are empty
      if (axy_combo_bodice_size == "" || axy_combo_bodice_size == undefined) {
        (popupmodal).find('tr.detail-line-bodice-size').hide();
      }

      if (axy_combo_skirt_size == "" || axy_combo_skirt_size == undefined) {
        (popupmodal).find('tr.detail-line-skirt-size').hide();
      }

      if (axy_combo_skirt_length == "" || axy_combo_skirt_length == undefined) {
        (popupmodal).find('tr.detail-line-skirt-length').hide();
      }

      if (current_product_size == "" || current_product_size == undefined) {
        (popupmodal).find('tr.detail-line-size').hide();
      }

      if (current_product_length == "" || current_product_length == undefined) {
        (popupmodal).find('tr.detail-line-length').hide();
      }

      if (current_product_color == "" || current_product_color == undefined) {
        (popupmodal).find('tr.detail-line-color').hide();
      }
            
      // Success feedback
      displayUserFeedbackMessage('success', 'Success!', 'Product added to Bag.');
      // Display modal
      $('html,body').css("overflow-y", "hidden");
      $('.md-modal').addClass('md-show');
    });
  }

  //get the value from the URL parameters
  function gup( name, url ) {
    if (!url) url = location.href;
    name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
    var regexS = "[\\?&]"+name+"=([^&#]*)";
    var regex = new RegExp( regexS );
    var results = regex.exec( url );
    return results == null ? null : results[1];
	}
</script>

